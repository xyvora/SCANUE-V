{
  "extends": [
    "next/core-web-vitals",
    "next"
  ],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_",
        "caughtErrorsIgnorePattern": "^_"
      }
    ],
    "semi": ["error", "always"]
  }
} 